{
  "name": "react-redux-typescript",
  "version": "1.0.0",
  "description": "Proof of Concept: React + Redux + Typescript using browserify + tsify to contain everything.",
  "scripts": {
    "lint": "tslint src/**/*.ts*",
    "browserify": "browserify src/main.tsx -p [ tsify --jsx react --noImplicitAny ] > out/bundle.js",
    "pack": "npm run lint && npm run browserify || echo ---"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ballantyno/reactReduxTypescript.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ballantyno/reactReduxTypescript/issues"
  },
  "homepage": "https://github.com/ballantyno/reactReduxTypescript#readme",
  "devDependencies": {
    "browserify": "^13.1.0",
    "tsify": "^1.0.3",
    "tslint": "^3.14.0",
    "typescript": "^1.8.10"
  },
  "dependencies": {
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "redux": "^3.5.2"
  }
}
